<div class="page-header">
  <h1><%= @review.summary %></h1>
</div>

<dl class="dl-horizontal">

  <dt>Book</dt>
  <dd><%= @review.book.title %> by <%= @review.book.author.fullName %></dd>


  <dt>User</dt>
  <dd><%= @review.user.username %></dd>


<!--   <dt>Summary</dt>
  <dd><%= @review.summary %></dd> -->


  <dt>Fullreview</dt>
  <dd><%= @review.fullReview %></dd>


  <dt>Startdate</dt>
  <dd><%= @review.startDate %></dd>


  <dt>Enddate</dt>
  <dd><%= @review.endDate %></dd>


  <dt>Enlightenmentrating</dt>
  <dd><%= @review.enlightenmentRating %></dd>


  <dt>Enjoymentrating</dt>
  <dd><%= @review.enjoymentRating %></dd>


  <dt>Overallrating</dt>
  <dd><%= @review.overallRating %></dd>


</dl>
